  ],
  "files": [],
  "workspaces": [],
  "lint-staged": {
    "*.{js,ts,cjs,mjs}": "eslint --fix",
    "*.{css,scss}": "prettier --write"
  },
  "devDependencies": {},
  "dependencies": {},
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
