{
  "name": "@fed-dev/eslint-config",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --fix --ext .js,.jsx,.cjs,.mjs,.ts,.tsx",
    "prettier": "prettier --write .",
    "stylelint": "stylelint \"**/*.{vue,scss,css}\" \"!**/dist/**\" --fix",
    "format": "npm run lint && npm run stylelint",
    "test": "vitest"
